{
  "name": "osiris",
  "version": "0.0.24",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:server\"",
    "dev:web": "ng serve --port 4500 --ssl --proxy-config ./proxy.config.json",
    "dev:server": "nodemon server/main.ts --watch server",
    "dev:mobile": "ng serve --host 192.168.1.246 --port 4500 --ssl --proxy-config ./proxy.config.json",
    "build": "npm run build:client; npm run build:server",
    "build:client": "ng build",
    "build:server": "tsc --project ./server/tsconfig.json",
    "postinstall": "bash postinstall.sh"
  },
